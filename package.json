{
  "name": "web-resume",
  "version": "1.2.0",
  "description": "The project is a web resume for easy demonstration of the basic skills of a software engineer.",
  "private": true,
  "author": "AlexEvs",
  "bugs": {
    "url": "https://github.com/a1exevs/web-resume/issues"
  },
  "keywords": [
    "resume",
    "cv",
    "react",
    "matrix",
    "skills",
    "experience"
  ],
  "scripts": {
    "dev": "react-app-rewired start",
    "build:local": "set PUBLIC_URL= && react-app-rewired build",
    "build": "react-app-rewired build",
    "predeploy": "yarn run format && yarn run lint && yarn run build",
    "deploy": "gh-pages -d build",
    "serve": "serve -s build",
    "check-deps": "yarn outdated",
    "upgrade-deps": "yarn upgrade --latest",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,scss}\"",
    "lint": "eslint --config eslint.config.ts \"{src,.storybook,linter,tests,scripts}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,.storybook,linter,tests,scripts}/**/*.{ts,tsx}\" --fix",
    "test:unit": "jest --config tests/unit/config/config.ts",
    "test:unit-cov": "jest --config tests/unit/config/config.ts --coverage",
    "test:snap": "jest --config tests/snap/config/config.ts",
    "test:snap-cov": "jest --config tests/snap/config/config.ts --coverage",
    "test:snap-update": "jest --updateSnapshot --config tests/snap/config/config.ts",
    "test:e2e": "playwright test --config=tests/e2e/config/config.ts",
    "test": "yarn test:unit && yarn test:snap && yarn test:e2e",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "docs": "typedoc",
    "version:major": "ts-node -P tsconfig.scripts.json scripts/increase-version.ts major",
    "version:minor": "ts-node -P tsconfig.scripts.json scripts/increase-version.ts minor",
    "version:patch": "ts-node -P tsconfig.scripts.json scripts/increase-version.ts patch",
    "update-version:major": "./scripts/update-version.sh major",
    "update-version:minor": "./scripts/update-version.sh minor",
    "update-version:patch": "./scripts/update-version.sh patch",
    "e2e:install": "playwright install --with-deps",
    "postinstall": "husky"
  },
  "dependencies": {
    "@alexevs/ts-guards": "1.0.9",
    "classnames": "2.5.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "7.10.1",
    "react-scripts": "5.0.1",
    "web-vitals": "3.5.2"
  },
  "devDependencies": {
    "@alexevs/prettier-config": "1.0.0",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@chromatic-com/storybook": "3.2.7",
    "@eslint/js": "9.39.1",
    "@playwright/test": "1.57.0",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/preset-create-react-app": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-webpack5": "8.6.14",
    "@storybook/test": "8.6.14",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/node": "20.19.26",
    "@types/react": "18.3.27",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "dotenv": "17.2.3",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-storybook": "0.12.0",
    "eslint-plugin-tsdoc": "0.5.0",
    "eslint-plugin-unused-imports": "4.3.0",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jiti": "2.6.1",
    "lint-staged": "13.3.0",
    "prettier": "2.8.8",
    "react-app-rewired": "2.2.1",
    "sass": "1.95.0",
    "serve": "14.2.5",
    "storybook": "8.6.14",
    "storybook-addon-pseudo-states": "4.0.4",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typedoc": "0.28.15",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
    "webpack-bundle-analyzer": "4.10.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.12.0"
}
